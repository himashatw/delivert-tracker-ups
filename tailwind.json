{
	"absolute": {
		"style": {
			"position": "absolute"
		}
	},
	"block": {
		"style": {
			"display": "block"
		}
	},
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"hidden": {
		"style": {
			"display": "none"
		}
	},
	"h-full": {
		"style": {
			"height": "100%"
		}
	},
	"flex-1": {
		"style": {
			"flexGrow": 1,
			"flexShrink": 1,
			"flexBasis": "0%"
		}
	},
	"items-center": {
		"style": {
			"alignItems": "center"
		}
	},
	"rounded-full": {
		"style": {
			"borderTopLeftRadius": 9999,
			"borderTopRightRadius": 9999,
			"borderBottomRightRadius": 9999,
			"borderBottomLeftRadius": 9999
		}
	},
	"bg-blue-200": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(191 219 254 / var(--tw-bg-opacity))"
		}
	},
	"px-3": {
		"style": {
			"paddingLeft": 12,
			"paddingRight": 12
		}
	},
	"py-1": {
		"style": {
			"paddingTop": 4,
			"paddingBottom": 4
		}
	},
	"pt-12": {
		"style": {
			"paddingTop": 48
		}
